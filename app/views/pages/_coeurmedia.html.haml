#myModalCoeur.modal.fade{:role => "dialog", :tabindex => "-1"}
  .modal-dialog{:role => "document"}
    .modal-content
      .modal-header
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
          %span{"aria-hidden" => "true"} ×
        %h4.modal-title Photothèque au cœur de l'action
      .modal-body
        #modalCarouselCoeur.carousel.slide
          .carousel-inner.carousel-inner_coeur
        %a.carousel-control.left{"data-slide" => "prev", :href => "#modalCarouselCoeur"}
          %i.glyphicon.glyphicon-chevron-left
        %a.carousel-control.right{"data-slide" => "next", :href => "#modalCarouselCoeur"}
          %i.glyphicon.glyphicon-chevron-right
